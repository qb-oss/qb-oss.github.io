{
  "version": "1.0",
  "manifest_version": 3,
  "name": "Endless",
  "description": "Overrides the browsers newtab page with a custom startpage",

  "icons": {
    "16": "assets/icons/home_icon_16.png",
    "32": "assets/icons/home_icon_32.png",
    "48": "assets/icons/home_icon_48.png",
    "64": "assets/icons/home_icon_64.png",
    "128": "assets/icons/home_icon_128.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
